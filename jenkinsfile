
pipeline {
    agent any
    tools { 
      	jdk "jdk11"
    }
    stages {
        stage('Cleanup Workspace') {
            steps {
                echo "Cleaned Up Workspace for ${APP_NAME}"
            }
        }


        stage('Code Build and run') {
            steps {
                 bat 'javac Hello.java'
                 bat 'java Hello'
            }
        }
		}   
}
